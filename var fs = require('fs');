var fs = require('fs');
fs.readFile('/path', function(err, file){
	console.log('读取文件完成');
});
console.log('开始读取文件');




var begin_timestamp = Date.now()/1000;
for(var i=0;i<10;++i){
	var data = fs.readFileSync('file.txt', 'utf-8');
}
var end_timestamp = Date.now()/1000;

console.log('average cost time: '+ (end_timestamp - begin_timestamp)/10);
